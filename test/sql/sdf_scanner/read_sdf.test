CHEBI:598	1-alkyl-2-acylglycerol	3	*C(=O)OC(CO)CO[1*]

# read_sdf without COLUMNS parameter should give helpful error
statement error
SELECT * FROM read_sdf('test/sql/sdf_scanner/test_sdf.sdf');
----
read_sdf requires a COLUMNS parameter

# empty SDF file with read_sdf_auto returns empty result with mol column
query I
SELECT * FROM read_sdf_auto('test/sql/sdf_scanner/empty.sdf');
----

# empty SDF file with explicit COLUMNS returns empty result
query II
SELECT * FROM read_sdf('test/sql/sdf_scanner/empty.sdf', COLUMNS={'id': 'VARCHAR', 'mol': 'Mol'});
----

# case-insensitive Mol type specification (MOL, mol, Mol all work)
query II
